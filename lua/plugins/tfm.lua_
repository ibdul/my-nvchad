return {
  "rolv-apneseth/tfm.nvim",
  lazy = false,
  opts={
    file_manager = "ranger",
  },
  config = function()
    -- Set keymap so you can open the default terminal file manager (yazi)
    vim.api.nvim_set_keymap("n", "<leader>d", "", {
      noremap = true,
      callback = require("tfm").open,
    })
  end,
}
